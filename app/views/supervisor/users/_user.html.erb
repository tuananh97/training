<% @users.each_with_index do |user| %>
  <tr>
    <td>1</td>
    <td><%= user.name %></td>
    <td><p><%= user.email %></p></td>
    <td><p><%= user.address %></p></td>
    <td><%= user.phone %></td>
    <% if current_user.supervisor? %>
      <td>
        <%= link_to edit_supervisor_user_path(user) do %>
          <i class="fa fa-pencil-square-o data-toogle="tooltip"
            title="<%= t '.edit' %>""></i>
        <% end %>&nbsp;
        <%= link_to supervisor_user_path(user), method: :delete,
          data: {confirm: t(".confirm")} do %>
          <i class="fa fa-trash-o data-toogle="tooltip"
            title="<%= t '.delete' %>""></i>
        <% end %>&nbsp;
        <%= link_to trainee_user_path(user) do %>
          <i class="fa fa-book" data-toogle="tooltip"
            title="<%= t '.view' %>"></i>
        <% end %>
      </td>
    <% end %>
  </tr>
<% end %>
