<% if user_signed_in? %>
  <%= form_for @test, url: user_tests_path(current_user), method: :post do |f| %>
    <%= f.hidden_field :exam_id, autofocus: true %>
    <%= f.hidden_field :status, value: :waitting, autofocus: true %>
    <%= f.submit t(".create_demo_test"), class: "btn btn-primary ui-tooltip", title: t(".create_test") %>
  <% end %>
<% end %>
